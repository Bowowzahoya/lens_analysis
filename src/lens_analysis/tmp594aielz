from .constants import *

def guess_aliases(app_df, apps):
    alias_sr = pd.Series(name="Alias")
    alias_sr.index.name = "Name"
    for app in apps:
        joint_apps = app_df.loc[app, JPW_COL]
        joint_apps = joint_apps.split(SEP)
        print(joint_apps)
        
        